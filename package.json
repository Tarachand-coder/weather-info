{
  "name": "weather-info",
  "version": "1.0.0",
  "description": "This is sukoon project",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf dist/*",
    "cacheClean": "npm cache verify",
    "tsc": "tsc",
    "build": "npm-run-all cacheClean clean lint:fix tsc copyViewsFolder",
    "dev": "node_modules src/index.ts",
    "develop": "npx cross-env NODE_ENV=local nodemon --watch src --ignore dist --exec 'ts-node-dev --respawn --notify false --prefer-ts src/index.ts'",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "copyViewsFolder": "mkdir dist\\views && xcopy src\\views\\* dist\\views\\ /E /I",
    "db:seed": "NODE_ENV=local ts-node-dev --notify false --prefer-ts --ignore-watch node_modules src/seeds/index.ts"
  },
  "license": "ISC",
  "author": "Sukoon",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.632.0",
    "@types/mocha": "^10.0.6",
    "axios": "^1.8.4",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "credit-card-type": "^9.1.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-useragent": "^1.0.15",
    "express-validation": "^4.1.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "mimemessage": "^1.0.5",
    "mocha": "^10.4.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongoose": "^8.3.3",
    "multer": "^1.4.5-lts.1",
    "node-html-parser": "^6.1.13",
    "nodemon": "^3.1.0",
    "openai": "^4.90.0",
    "sprintf-js": "^1.1.3",
    "ts-node-dev": "^2.0.0",
    "twilio": "^3.71.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express-useragent": "^1.0.5",
    "@types/jsonwebtoken": "^9.0.9",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
